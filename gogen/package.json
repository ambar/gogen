{
  "name": "gogen",
  "version": "1.1.0",
  "description": "Use stream API to scaffold projects or files",
  "repository": "ambar/gogen",
  "license": "MIT",
  "source": "lib/index.js",
  "main": "dist/index.js",
  "bin": "cli.js",
  "engines": {
    "node": ">= 10"
  },
  "files": [
    "dist",
    "index.js",
    "cli.js"
  ],
  "publishConfig": {
    "main": "dist"
  },
  "keywords": [
    "gogen",
    "generator",
    "initializer",
    "starter kit",
    "scaffolding",
    "sao",
    "yeoman"
  ],
  "scripts": {
    "prepublishOnly": "npm run build",
    "prebuild": "rm -rf dist",
    "build": "yarn build:esbuild",
    "build:ncc": "ncc build lib -o dist -m",
    "build:webpack": "webpack",
    "build:esbuild": "esbuild $npm_package_source --outfile=$npm_package_main --bundle --minify --platform=node --target=node10 --format=cjs",
    "build:inspect": "SOURCE_MAP=yes yarn build && source-map-explorer dist/index.js"
  },
  "devDependencies": {
    "get-stream": "^4.1.0",
    "kleur": "^3.0.0",
    "lodash.template": "^4.4.0",
    "mri": "^1.2.0",
    "prompts": "^1.2.1",
    "through2": "^2.0.0",
    "vinyl-fs": "^3.0.3"
  }
}

{
  "name": "gogen",
  "version": "0.0.5",
  "description": "Use stream API to create generator/initializer packages",
  "repository": "ambar/gogen",
  "license": "MIT",
  "main": "dist",
  "bin": "gogen.js",
  "engines": {
    "node": ">= 8"
  },
  "files": [
    "dist",
    "gogen.js"
  ],
  "keywords": [
    "generator",
    "initializer",
    "starter kit",
    "scaffolding",
    "yeoman"
  ],
  "scripts": {
    "prepublishOnly": "npm run build:webpack && npm run test",
    "clean": "rm -rf dist",
    "build:ncc": "npm run clean && npx @zeit/ncc build lib -S -o dist",
    "build:webpack": "npm run clean && webpack -p",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch --notify"
  },
  "devDependencies": {
    "get-stream": "^4.1.0",
    "jest": "^23.6.0",
    "kleur": "^3.0.0",
    "lodash.template": "^4.4.0",
    "minimist": "^1.2.0",
    "prompts": "^1.2.1",
    "pump": "^3.0.0",
    "through2": "^2.0.0",
    "vinyl-fs": "^3.0.3",
    "webpack": "^4.28.0",
    "webpack-cli": "^3.1.2"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/examples/"
    ],
    "modulePathIgnorePatterns": [
      "/examples/"
    ]
  }
}
